<?xml version="1.0"?>
<data>
  <xpath expr="/form/notebook/page/field[@name='amount']"
	 position="after">
    <label name="due_date" />
    <field name="due_date" />
    <label name="due_date_postfix" />
    <field name="due_date_postfix" />
    <label name="folder_no" />
    <field name="folder_no" />
    <newline />
    <label name="folder_subno" />
    <field name="folder_subno" />
    <label name="folder_submax" />
    <field name="folder_submax" />
    <label name="proj_line0" />
    <field name="proj_line0" />
    <label name="proj_line1" />
    <field name="proj_line1" />
    <label name="proj_line2" />
    <field name="proj_line2" />
    <newline />
    <label name="material" />
    <field name="material" />
    <label name="sheet_thickness" />
    <field name="sheet_thickness" />
    <label name="material_extra" />
    <field name="material_extra" />
    <label name="material_surface" />
    <field name="material_surface" />
    
    <separator name="invoice_lines" colspan="4"/>
    <label name="inv_skip" />
    <field name="inv_skip" />
    <newline />
    <label name="inv_line0" />
    <field name="inv_line0" />
    <label name="inv_line0_skip" />
    <field name="inv_line0_skip" />
    <label name="inv_line1" />
    <field name="inv_line1" />
    <newline />
    <label name="inv_line2" />
    <field name="inv_line2" />
    <label name="inv_line2_skip" />
    <field name="inv_line2_skip" />
  </xpath>
  <xpath expr="/form/notebook/page/field[@name='shipping_date']"
	 position="replace" />
  <xpath expr="/form/notebook/page/label[@name='shipping_date']"
	 position="replace" />
  <!-- TODO: maybe add elsewhere ? -->
  <xpath expr="/form/notebook" position="inside">
    <page string="Extra" col="2" id="extra">
      <label name="shipping_date"/>
      <field name="shipping_date"/>
    </page>
  </xpath>
</data>
