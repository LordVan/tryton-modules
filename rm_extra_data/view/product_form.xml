<?xml version="1.0" ?>
<data>
  <xpath expr="/form/notebook/page/label[@name='list_price']" position="before">
    <label name="material" />
    <field name="material" />
    <label name="sheet_thickness" />
    <field name="sheet_thickness" />
    <label name="material_extra" />
    <field name="material_extra" />
    <label name="material_surface" />
    <field name="material_surface" />
    <newline />
  </xpath>
  <xpath expr="/form/notebook" position="inside">
    <page string="Project / Invoice" col="4" id="proj_inv">
      <label name="real_product" />
      <field name="real_product" />
      <label name="folder_skip" />
      <field name="folder_skip" />
      <newline />
      <label name="folder_subno" />
      <field name="folder_subno" />
      <label name="folder_submax" />
      <field name="folder_submax" />
      <label name="proj_line0" />
      <field name="proj_line0" colspan="3" />
      <label name="proj_line1"/>
      <field name="proj_line1" colspan="3" />
      <label name="proj_line2" />
      <field name="proj_line2" colspan="3" />
      <label name="proj_impnote" />
      <field name="proj_impnote" colspan="3" />
      <newline />
      <label name="material" />
      <field name="material" />
      <label name="sheet_thickness" />
      <field name="sheet_thickness" />
      <label name="material_extra" />
      <field name="material_extra" colspan="3" />
      <label name="material_surface" />
      <field name="material_surface" colspan="3" />
      <separator name="invoice_lines" colspan="4" />
      <label name="inv_skip" />
      <field name="inv_skip" />
      <newline />
      <label name="inv_line0" />
      <field name="inv_line0" />
      <label name="inv_line0_skip" />
      <field name="inv_line0_skip" />
      <label name="inv_line1" />
      <field name="inv_line1" colspan="3" />
      <newline />
      <label name="inv_line2" />
      <field name="inv_line2" />
      <label name="inv_line2_skip" />
      <field name="inv_line2_skip" />
    </page>
  </xpath>
</data>
