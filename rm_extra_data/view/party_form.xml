<?xml version="1.0"?>
<data>
  <xpath expr="/form/notebook" position="inside">
    <page string="Extra" col="2" id="extra">
      <label name="salutation" />
      <field name="salutation" />
      <label name="legal_name" />
      <field name="legal_name" />
      <label name="inv_name_line1" />
      <field name="inv_name_line1" />
      <label name="inv_name_line2" />
      <field name="inv_name_line2" />
      <label name="customer_no" />
      <field name="customer_no" />
      <label name="supplier_no" />
      <field name="supplier_no" />
      <label name="pn_name" />
      <field name="pn_name" />
      <label name="sale_note" />
      <field name="sale_note" />
      <!-- usually not shown -->
      <label name="dolibarr_pid" />
      <field name="dolibarr_pid" />
      <label name="dolibarr_cid" />
      <field name="dolibarr_cid" />
      <!--      <label name="place_of_jurisdiction" />
	   <field name="place_of_jurisdiction" />-->
    </page>
  </xpath>
  <xpath expr="/form/notebook/page[@id='sale']" position="inside">
    <separator string="RM Extra" colspan="4" id="rm_extra" />
    <label name="accept_exact_amounts" />
    <field name="accept_exact_amounts" />
    <label name="charge_setup_costs" />
    <field name="charge_setup_costs" />

  </xpath>
</data>
